algoritmo "CONVERSOR DE MOEDAS"
var
 C, Q: inteiro
 K, D: Real

inicio
  C <- 1
  Escreva(" Quantas vezes voce quer converter: ")
  Leia(Q)
  Enquanto C <= Q Faca
            Escreva("Qual o valor em KZ : ",  " AOA" )
            Leia(K)
            D <- k/661
            Escreval("O valor convertido sera: ", D:5:2)
            C <- C + 1
 FimEnquanto

fimalgoritmo